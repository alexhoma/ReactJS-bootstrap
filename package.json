{
  "name": "alexhoma-react",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "react-scripts": "0.9.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1"
  },
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html public/index.html && webpack-dev-server --content-base src/ --inline --hot --port 3000 --history-api-fallback",
    "build:prod": "webpack -p && cp src/index.html public/index.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
